{
"parser": "babel-eslint",
"env": {
    "node": true,
    "es6": true,
    "jasmine": true,
    "mocha": true
  },
  "globals": {
      "angular": true
  },
"ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "modules": true
  },
"rules": {
    "no-console": 1,
    "no-debugger": 2,
    "quotes": [1, "single", "avoid-escape"],
    "no-shadow": [0],
    "no-underscore-dangle": [0],
    "strict": [1, "never"],
    "no-spaced-func": [1],
    "space-after-keywords": [1],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-unused-expressions": [0],
    "no-use-before-define": [0],
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-labels": 0
  }
}
